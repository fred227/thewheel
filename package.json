    "browserslist": "> 0.5%, last 2 versions, not dead",
    "license": "Apache-2.0",
    "engines": {
        "node": "16.x",
        "yarn": "1.x",
        "npm": "8.x"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "src",
        ".editorconfig",
        ".eslintignore",
        ".eslintrc.json",
        ".gitignore",
        ".prettierignore",
        ".prettierrc",
        "LICENSE",
        "package.json",
        "tsconfig.json"
    ],
    "scripts": {
        "clean": "shx rm -rf dist .parcel-cache",
        "build": "yarn clean && parcel build src/index.html",
        "start": "yarn clean && parcel src/index.html",
        "fmt": "prettier --write '{*,**/*}.{js,ts,jsx,tsx,json}'",
        "lint": "eslint . && prettier --check '{*,**/*}.{js,ts,jsx,tsx,json}'",
        "lint:fix": "eslint --fix . && yarn fmt",
        "nuke": "shx rm -rf node_modules yarn.lock",
        "heroku-postbuild": "parcel build src/index.html"
    },
    "dependencies": {
        "@babel/core": "^7.17.5",
        "@babel/runtime": "^7.17.2",
        "@emotion/react": "^11.0.0",
        "@emotion/styled": "^11.0.0",
        "@mui/icons-material": "^5.0.0",
        "@mui/lab": "^5.0.0-alpha.69",
        "@mui/material": "^5.0.0",
        "@project-serum/anchor": "^0.21.0",
        "@solana/wallet-adapter-base": "^0.9.3",
        "@solana/wallet-adapter-material-ui": "^0.16.4",
        "@solana/wallet-adapter-react": "^0.15.3",
        "@solana/wallet-adapter-wallets": "^0.15.0",
        "@solana/web3.js": "^1.31.0",
        "notistack": "^2.0.0",
        "react": "^17.0.0",
        "react-countdown": "^2.3.2",
        "react-dom": "^17.0.0",
        "react-minimal-pie-chart": "^8.2.0",
        "react-router-dom": "^6.2.1",
        "yarn": "^1.22.17",
        "shx": "^0.3.4",
        "parcel": "^2.3.2"
    },
    "devDependencies": {
        "@types/eslint": "^8.2.2",
        "@types/eslint-plugin-prettier": "^3.1.0",
        "@types/node": "^16.11.14",
        "@types/prettier": "^2.4.3",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "parcel": "^2.3.2",
        "prettier": "^2.5.1",
        "shx": "^0.3.4",
        "tslib": "^2.3.1",
        "typescript": "^4.5.4"
    }
}
